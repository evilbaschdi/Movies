<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>$([System.DateTime]::UtcNow.ToString(yyyy.M.d.Hmm))-develop</Version>
    <Authors>Sebastian Walter</Authors>
    <Company>evilbaschdi (Sebastian Walter)</Company>
    <Product>Movie</Product>
    <Copyright>Copyright © 2014 - $([System.DateTime]::UtcNow.ToString(yyyy)) evilbaschdi</Copyright>
    <LangVersion>8.0</LangVersion>
    <PackageProjectUrl>https://github.com/evilbaschdi/movies</PackageProjectUrl>
    <RepositoryUrl>https://github.com/evilbaschdi/movies</RepositoryUrl>
    <PackageIconUrl>https://github.com/evilbaschdi/Movies/blob/master/Movie/movie.ico</PackageIconUrl>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\Movie.Core.xml</DocumentationFile>
  </PropertyGroup>


  <ItemGroup>
    <None Remove="newMovieDb.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="newMovieDb.xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="EvilBaschdi.Core" Version="2020.6.12.1028" />
    <PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0-preview.6.20305.6" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Models\MovieRecord.cs">
      <DependentUpon>IMovieRecord.cs</DependentUpon>
    </Compile>
    <Compile Update="Movies.cs">
      <DependentUpon>IMovies.cs</DependentUpon>
    </Compile>
    <Compile Update="ResourceStreamText.cs">
      <DependentUpon>IResourceStreamText.cs</DependentUpon>
    </Compile>
    <Compile Update="XmlDatabase.cs">
      <DependentUpon>IXmlDatabase.cs</DependentUpon>
    </Compile>
    <Compile Update="XmlSettings.cs">
      <DependentUpon>IXmlSettings.cs</DependentUpon>
    </Compile>
  </ItemGroup>

</Project>